/* sorting of input string
#include<stdio.h>
#include<string.h>
int main()
{
  int i,j,count;
  char str[25][25],temp[25];
  puts("Strings to be entered?");
  scanf("%d",&count);
  puts("enter string to be entered");
  for(i=0;i<=count;i++)
   get(str[i]);
  for(i=0;i<=count;i++)
    for(j=i+1;j<=count;j++)
      {
      if(strcmp(str[i],str[i],str[j])>0
        {
        strcpy(temp,str[i]);
        strcpy(str[i],str[j]);
        strcpy(str[j],temp);
        }
      }
   printf("order of srted array:");
   for(i=0;i,=count;i++)
    puts(str[i]);
   return 0;
}
    
  
